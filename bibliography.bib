}

@article{yeDailyAccessedStreet2019,
  title = {Daily {{Accessed Street Greenery}} and {{Housing Price}}: {{Measuring Economic Performance}} of {{Human}}-{{Scale Streetscapes}} via {{New Urban Data}}},
