  file = {/Volumes/Samsung_T5/Zotero/storage/W7MRLYJQ/Martino et al. - 2021 - Urban form and livability socioeconomic and built.pdf;/Volumes/Samsung_T5/Zotero/storage/27HS6SJE/bc.82.html},
