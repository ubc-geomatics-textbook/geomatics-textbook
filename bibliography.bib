@techreport{glaeserNowcastingLocalEconomy2017,
  title = {Nowcasting the {{Local Economy}}: {{Using Yelp Data}} to {{Measure Economic Activity}}},
  shorttitle = {Nowcasting the {{Local Economy}}},
