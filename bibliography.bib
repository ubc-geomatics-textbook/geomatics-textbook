@article{luIntegrationEnergyEconomy2021,
  title = {Integration of an Energy\textendash{} Economy Model with an Urban Energy Model},
  author = {Lu, Yuhao and Scott, Alex and Kim, Juchan and Curi, Camila Barbosa and McCarty, Justin and Pardy, Aaron and Rysanek, Adam and Girling, Cynthia and Kellett, Ronald},
