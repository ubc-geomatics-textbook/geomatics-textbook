  language = {en},
  number = {3}
}

@misc{mortensenDoesCommunityAmenity2020,
  title = {Does the {{Community Amenity Charge Rachet Skew}} Our {{Housing Market}}? | {{LinkedIn}}},
  author = {Mortensen, Michael},
  year = {2020},
  abstract = {When cities set "Community Amenity Contributions" based on zenith sales price assumptions, are they unintentionally skewing their housing markets to luxury submarkets over more domestic ones? I think this has been the case in Vancouver and Metro Vancouver. Many cities charge developers for new developable area when they rezone properties. In effect, Cities are clawing back the land lift associated with new development rights and intensities granted through rezonings. The practice in the City of Vancouver (and increasingly other municipalities) is that the City seeks to extract 75\% of the land lift, leaving a bit on the table to cover the developer's expenses to assemble and rezone etc. Many land economists argue that negotiated Community Amenity Contributions do not impact housing end-prices. They assert that the land lift extracted by Cities as CAC's ``come out of land prices''. However, the reality (and problem) is that most CAC's are negotiated by Cities long after developers buy land, and often at the very end of the rezoning process. One has to guess what the CAC is going to be at the time of the land purchase; there are no opportunities to go back and renegotiate your original purchase price if the City gives you a CAC surprise. The CAC process is not transparent or predictable and the actual CAC demanded by Cities - predicated on current zenith housing sale prices - can be significantly higher than the developer's going-in assumption. Note that power in these negotiations is very asymmetrical: Council has absolute power to approve or reject rezoning applications. Many developers recover from `CAC surprises' by swallowing the additional cost and reorienting their product to higher-end segments of the market, increasing sales prices and setting in the process NEW zenith sale prices which in turn rachet up City's CAC expectations for rezoning applications that follow. That's the "Rachet" pictured above. This economic machinery works generally in a rising market if one ignores the increasing unaffordability of new product for local incomes and local buyers. However, the cracks in the business model appear when the market is moving in opposite direction. A recent article in the Globe and Mail highlight's Vancouver's over-reliance on Community Amenity Contributions to fund core City Amenities. See: ttps://www.theglobeandmail.com/canada/british-columbia/article-flow-of-money-from-builders-for-community-amenities-in-vancouver-dries/ Many projects are stalled because the CACs demanded by local government - predicated on old zenith sales prices - are not supportable when markets weaken. To be clear, I agree with the principle that new land use intensities should be accompanied with commensurate increases in local amenities funded by development. And while a City-Wide review of land use is well overdue to reset development options under existing land use zoning (and avoid lengthy rezonings), I believe that Vancouver's transactional and discretionary land use and rezoning system has worked and can continue to work for us, delivering an array of new housing, shops, amenities and good urban design and placemaking. I am questioning the practice where zenith sales prices are used to set CACs because these have eclipsed the capacity of local incomes. CACs should be reasonable and should not impair economic development options and they should not be set so high that one cannot sell new homes at prices that are supportable based on local incomes. Generally speaking, new development ALREADY pays its fair share towards the cost of servicing new growth even before the extraction of CACs. Consider City DCL's at \$28/sf Gross Floor Area; City Engineering Charges for Off-Site upgrades (sometimes six-figures); Metro DCCs at \$1072/apt; Translink DCCs at \$1545/unit; Public Art at \$2/sf GFA etc). Note that CAC's are pretty much off the table for rental housing because the end rents needed to support such increased land values are so far off the charts that even local governments admit, grudgingly, that any CAC of any kind is not supportable. The numbers don't lie. Is there some hope that we can have a rational approach on the CAC issue? Can we have more reasonable CAC demands and a more transparent CAC process? One very interesting recent land negotiation has set an interesting precedent but it has not received a lot of attention: the South False Creek Land Lease renewals. Few people have taken notice that the basis for the City's lease renewal valuation was set based on the shelter payments predicated on local incomes vs the zenith of the absolute ``highest and best use''. This got me wondering: "What if CACs were more transparent across the City and Region, and what if they were set based on projected sales prices and land residuals informed by what people living and working in Vancouver can afford?" Let's start that discussion! Michael Mortensen, MA MCIP. Director: Liveable CITY Planning Ltd. / Cell (778) 899-7943 / Vancouver BC Canada / www.LiveableCityPlanning.com  Director: www.LAI.org . www.Urbanarium.org . Vancouver Farmers Markets . Small Housing BC . Vancouver Short Track Speedskating . Member: GNW Trust Real Estate Committee (c) 2020},
  file = {/Volumes/Samsung_T5/Zotero/storage/I6IKBZUA/does-community-amenity-charge-rachet-skew-our-housing-mortensen.html}
}

@article{mosgenStateledGentrificationPreviously2019,
  title = {State-Led Gentrification in Previously `Un-Gentrifiable' Areas: {{Examples}} from {{Vancouver}}/{{Canada}} and {{Frankfurt}}/{{Germany}}},
  shorttitle = {State-Led Gentrification in Previously `Un-Gentrifiable' Areas},
  author = {M{\"o}sgen, Andrea and Rosol, Marit and Schipper, Sebastian},
  year = {2019},
  month = oct,
  volume = {26},
  pages = {419--433},
  publisher = {{SAGE Publications Ltd}},
  issn = {0969-7764},
  doi = {10.1177/0969776418763010},
  abstract = {Through an analysis of two international cases from Canada and Germany, this paper highlights the role of the state in governing gentrification and displacement in areas previously thought to be unattractive for profit-seeking capital, that is, `un-gentrifiable'. With this, we seek to contribute to the debate on how the role of the local state has changed from securing affordable housing for low-income households into becoming an essential player involved in real estate speculation. Taking Little Mountain in Vancouver as the first example, we examine the privatization and demolition of the public housing complex and thus the withdrawal of the state. Our second example, Ostend in Frankfurt, investigates the restructuring of a working-class neighbourhood through active state-led interventions including massive public investment. We analyse the two empirical examples along five dimensions: causal drivers and mechanisms that have led to the changing role of the state in governing urban transformations; policy instruments used by state agencies to encourage gentrification; strategies to legitimize state-led gentrification; outcomes in terms of direct and exclusionary displacement; and the forms of contestation and protest. We maintain that both cases, although presenting a stark contrast, follow the same rule, namely state-led gentrification.},
  annotation = {2 citations (Crossref) [2021-01-16]},
  file = {/Volumes/Samsung_T5/Zotero/storage/YT8WTEK4/Mösgen et al. - 2019 - State-led gentrification in previously ‘un-gentrif.pdf},
  journal = {European Urban and Regional Studies},
  keywords = {Frankfurt,Gentrification,housing,local state,social mixing,Vancouver},
  language = {en},
  number = {4}
}

