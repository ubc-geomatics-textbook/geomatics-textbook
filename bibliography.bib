@article{tenorioAoDesocupadoEm2012,
  title = {{Ao desocupado em cima da ponte. Bras\'ilia, arquitetura e vida p\'ublica}},
  author = {Tenorio, Gabriela de Souza},
  year = {2012},
  month = jun,
  abstract = {Esta tese trata de espa\c{c}os p\'ublicos e como adequa-los/cria-los para favorecer a vida  p\'ublica: o conjunto das a\c{c}\~oes, programadas ou n\~ao, protagonizadas por indiv\'iduos nos espa\c{c}os  p\'ublicos. Para isso, estabelece a rela\c{c}\~ao entre arquitetura e vida p\'ublica, considerando a  arquitetura como vari\'avel independente e realidade que pode ser examinada sob v\'arias lentes,  sendo uma delas a sociol\'ogica (sua maior ou menor capacidade de favorecer a urbanidade),  tendo como ponto de partida a Teoria da Sintaxe Espacial. Em seguida, classifica as atividades  no espa\c{c}o p\'ublico, do qual tra\c{c}a um breve hist\'orico da cidade da Revolu\c{c}\~ao Industrial ao est\'agio  atual da cidade contempor\^anea. Apresenta os achados sobre a vida p\'ublica de autores que  se preocuparam em compreender o funcionamento das pessoas nos lugares, e extrai deles  t\'ecnicas de levantamento da vida p\'ublica, sugest\~oes de itens de verifica\c{c}\~ao e de elementos de  configura\c{c}\~ao de espa\c{c}os p\'ublicos que favore\c{c}am a vida p\'ublica. A partir da\'i oferece um m\'etodo  para se conhecer, saber observar, avaliar e, consequentemente, manipular, os principais  atributos de um espa\c{c}o p\'ublico incidentes no seu desempenho sociol\'ogico. Tendo Bras\'ilia como  objeto de estudo, realiza uma cr\'itica \`a cidade, ao mostrar que ela n\~ao s\'o ainda n\~ao vem  trabalhando plenamente para favorecer sua vida p\'ublica, como muitas vezes vem agindo de  forma a ampliar ainda mais as caracter\'isticas que a prejudicam. Ao mesmo tempo, mostra que  ela pode ser uma cidade que valoriza seus habitantes, ao oferecer-lhes espa\c{c}os p\'ublicos de  alt\'issima qualidade nos quais possa florescer a urbanidade. Caracteriza a estrutura de Bras\'ilia,  mostra como ela lida com os sujeitos e as atividades nos espa\c{c}os p\'ublicos, e com os elementos  de configura\c{c}\~ao influentes na vida p\'ublica. O m\'etodo proposto \'e aplicado especificamente  na Plataforma Rodovi\'aria e no Eixo Monumental leste, e as recomenda\c{c}\~oes dadas ao final de  cada an\'alise v\^em como contribui\c{c}\~ao \`a cidade, ao mostrar que \'e poss\'ivel Bras\'ilia ter uma vida  p\'ublica mais intensa mesmo em seus espa\c{c}os predominantemente simb\'olicos, e que isso n\~ao  macula as caracter\'isticas que a fazem \'unica no mundo e que lhe conferiram o t\'itulo de Patrim\^onio  Cultural da Humanidade. Pelo contr\'ario.   \_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_ ABSTRACT},
  copyright = {Acesso Aberto},
  file = {/Volumes/Samsung_T5/Zotero/storage/FYVIM9FV/Tenorio_2012_Ao desocupado em cima da ponte.pdf},
  keywords = {_tablet},
  language = {Portugu\^es}
}

