@article{loiblModelingGrowthDensification2003,
  title = {Modeling Growth and Densification Processes in Suburban Regions\textemdash Simulation of Landscape Transition with Spatial Agents},
  author = {Loibl, W. and Toetzer, T.},
  year = {2003},
