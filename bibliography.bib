  doi = {10.1080/10511482.2016.1163611},
  abstract = {Despite an overall decrease in residential mobility after the 2007 housing crisis, many households, particularly those that are low income, continue to move in pursuit of a better life. Traditional theories of residential mobility suggest that mobility will occur when housing and transportation costs are cumulatively greater than the cost of moving to a new location. At the same time, the influence of these factors is not likely to be uniform across geographic contexts or for moves up or down the metropolitan hierarchy. Our analysis examines how well affordability measures explain patterns of county-level residential mobility. Specifically, we contrast conventional measures of affordability focused on the ratio of income to housing expense with measures of location affordability that factor in both housing and transportation costs. We find that whereas households tend to move from lower to higher cost locations, transit affordability at the destination plays an important role in mobility decisions.},
  annotation = {\_eprint: https://doi.org/10.1080/10511482.2016.1163611},
  file = {/Volumes/Samsung_T5/Zotero/storage/T4SSI7IE/Greenlee and Wilson - 2016 - Where Does Location Affordability Drive Residentia.pdf;/Volumes/Samsung_T5/Zotero/storage/LKLSEWCG/10511482.2016.html},
  journal = {Housing Policy Debate},
  keywords = {location affordability,population change,Residential mobility},
  number = {4-5}
}

