@article{trounstineGeographyInequalityHow2020,
  title = {The {{Geography}} of {{Inequality}}: {{How Land Use Regulation Produces Segregation}}},
  shorttitle = {The {{Geography}} of {{Inequality}}},
  author = {Trounstine, Jessica},
  year = {2020},
