  file = {/Volumes/Samsung_T5/Zotero/storage/TTBKWSVB/Bency et al. - 2017 - Beyond Spatial Auto-Regressive Models Predicting .pdf},
  keywords = {autoregressive processes,Biological system modeling,Birmingham,CNN framework,convolutional neural network,Correlation,correlation methods,Data models,deep neural networks,Feature extraction,feedforward neural nets,geo-spatial data,geophysical image processing,housing prices prediction,image resolution,Liverpool,London,Mathematical model,neighborhood information,pricing,remote sensing,SAR baseline,satellite imagery multiple resolutions,Satellites,smoothing methods,spatial auto-regressive models,spatial correlations,spatial smoothing,Urban areas}
